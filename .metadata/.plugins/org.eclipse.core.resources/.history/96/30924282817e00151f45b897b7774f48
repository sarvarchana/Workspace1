package TestCases;

import org.junit.Test;
import org.openqa.selenium.By;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.support.ui.Select;

public class FacebookSignup {

	@Test
	public void FacebookSignin(){
		
		FirefoxDriver d = new FirefoxDriver();
		
		d.get("https://www.facebook.com/");
		
		d.findElement(By.id("u_0_b")).clear();
		d.findElement(By.id("u_0_b")).sendKeys();
		
		d.findElement(By.id("u_0_d")).clear();
		d.findElement(By.id("u_0_d")).sendKeys();
		
		d.findElement(By.id("u_0_f")).clear();
		d.findElement(By.id("u_0_f")).sendKeys();
		
		d.findElement(By.id("u_0_i")).clear();
		d.findElement(By.id("u_0_i")).sendKeys();
		
		
		d.findElement(By.id("u_0_k")).clear();
		d.findElement(By.id("u_0_k")).sendKeys();
		
		Select _Month = new Select(d.findElementById("month"));
		_Month.selectByVisibleText("");
		
		Select _Day = new Select(d.findElementById("day"));
		_Day.selectByValue("");
		
		
		Select _Year = new Select(d.findElementById("year"));
		_Year.selectByVisibleText("");
		
		
		
		
		
		
		
		
	}
}
